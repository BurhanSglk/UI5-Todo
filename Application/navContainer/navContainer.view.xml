<mvc:View
	controllerName="SapUI5Tutorial.Application.navContainer.controller.navContainer"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">
	<Page showHeader="false">
		<NavContainer 
			id="navCon"
			width="100%"
			height="50rem"
			class="sapUiSmallMarginBottom">
			<Page id="page"  height="50rem" width="50rem">                 
                   <mvc:XMLView viewName="SapUI5Tutorial.Application.addTicket.view.addTicket" height="50rem"/>
               </Page>
			<Page id="page1" showNavButton="true" navButtonPress="onNavBack" height="50rem" width="50rem" >
				<mvc:XMLView viewName="SapUI5Tutorial.Application.TicketList.view.TicketList" height="50rem"/>
			</Page>			
		</NavContainer>
	</Page>
</mvc:View>